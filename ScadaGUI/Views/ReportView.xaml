<UserControl x:Class="ScadaGUI.Views.ReportView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <StackPanel Margin="10">
        <Button Content="Generate Report" Command="{Binding GenerateReportCommand}" Width="200" Height="40" Margin="10"/>
        <TextBlock Text="{Binding ReportStatus}" Margin="10" Foreground="Green"/>
    </StackPanel>
</UserControl>